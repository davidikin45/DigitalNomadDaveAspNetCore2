@inject AppSettings appSettings

<div class="modal-header">
    <h3 class="modal-title">Location</h3>
</div>

<div class="modal-body">
    <div class="row">
        <div class="col-12">
            <iframe width="100%" height="350px" frameborder="0" style="border:0" src="{{vm.trustSrc('@(appSettings.GoogleMapsApiKey)')}}"></iframe> 
            <p>{{vm.latitude}},{{vm.longitude}}
            </p>
        </div>
    </div>
</div>

<div class="modal-footer">
    <button type="button" ng-click="$dismiss();" class="btn btn-primary">Done</button>
</div>
