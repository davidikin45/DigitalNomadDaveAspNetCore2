{
  "SwitchSettings": {
    "EnableHangfire": true
  },
  "detailedErrors": "true",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "Console",
        "Args": { "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} - {Message}{NewLine}{Exception}" }
      }
    ]
  },
  "ConnectionStrings": {
    "IdentityConnection": "Server=(localdb)\\MSSQLLocalDB;Database=IntegrationTests;Trusted_Connection=True;MultipleActiveResultSets=true;",
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=IntegrationTests;Trusted_Connection=True;MultipleActiveResultSets=true;",
    "HangfireConnection": "Server=(localdb)\\MSSQLLocalDB;Database=IntegrationTests;Trusted_Connection=True;MultipleActiveResultSets=true;"
  }
}
