{
  "detailedErrors": "true",
  "AppSettings": {
    "SiteUrl": ""
  },
  "SwitchSettings": {
    "EnableRedirectNonWwwToWww": false,
    "EnableRedirectHttpToHttps": true,
    "EnableETags": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "Console",
        "Args": { "outputTemplate": "[{Timestamp:HH:mm:ss.fff}] {Level:u3} - {Message}{NewLine}{Exception}" }
      }
    ]
  }
}
